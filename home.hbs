<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
<style>
    .center{
        display:block;
        margin-left:auto;
        margin-right: auto;
    }
</style>
<h1 style="text-align: center;">Covid19 data </h1>
{{#each info as |val key|}}
<h2 style="text-align:center">{{val.country}}</h2>
<h3 style="text-align:center" class="text-warning">Total Cases:{{val.cases}}</h3>
<h3 style="text-align:center" class="text-success">Total recovered:{{val.recovered}}</h3>
<h3 style="text-align:center" class="text-danger">Total Death:{{val.deaths}}</h3>
<img class="center" src="{{val.countryInfo.flag}}" /><br>
 {{/each}}